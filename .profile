ulimit -c 0
BROWSER="links"
SEARCH_ENGINE="duck.com"
LC_ALL=en_US.UTF-8
PATH=$PATH:$HOME/.local/bin:$HOME/.local/bin/statusbar
ENV=$HOME/.kshrc
HISTFILE="$HOME/.cache/histfile"
HISTSIZE=100
EDITOR=nvim
MANPATH="/usr/share/man:/usr/local/share/man:/usr/local/man"
export BROWSER SEARCH_ENGINE LC_ALL ENV PATH HISTFILE HISTSIZE EDITOR MANPATH
